<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.user_search_video">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/recent_search_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            android:src="?android:attr/actionModeWebSearchDrawable"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:ellipsize="end"
            android:maxLines="2"
            android:padding="7dp"
            android:paddingStart="10dp"
            android:paddingEnd="15dp"
            android:text="How to debug react-native applfsdf dssa Browser"
            android:textColor="@color/cpDark"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/recent_search_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/recent_search_icon"
            app:layout_constraintTop_toTopOf="@+id/recent_search_icon" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>