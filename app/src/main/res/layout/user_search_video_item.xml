<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/recent_search_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:src="?android:attr/actionModeWebSearchDrawable"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewVideoTitle"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:ellipsize="end"
        android:maxLines="2"
        android:padding="7dp"
        android:paddingStart="10dp"
        android:paddingEnd="15dp"
        android:text="How to debug react-native applfsdf dssa Browser"
        android:textColor="@color/cpDark"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/recent_search_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/recent_search_icon"
        app:layout_constraintTop_toTopOf="@+id/recent_search_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>